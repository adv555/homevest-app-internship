{
  "private": true,
  "engines": {
    "node": "14.x.x",
    "npm": "6.x.x"
  },
  "scripts": {
    "migrate": "sequelize-cli db:migrate",
    "db:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "db:seed": "bash generateSeed.sh",
    "schema:sync": "sudo ts-node ./node_modules/typeorm/cli.js schema:sync",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "start:dev": "nodemon --exec ts-node -r dotenv/config -r tsconfig-paths/register ./src/server.ts",
    "start": "node -r dotenv/config ./src/server.js",
    "build:ts": "tsc && tscpaths -p tsconfig.json --src . --out build",
    "build:copy": "cp -r shared build && cp -r public build",
    "build": "npm run build:ts && npm run build:copy"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.11",
    "aws-sdk": "^2.1077.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.4.1",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "nanoid": "3.1.20",
    "nodemailer": "^6.7.2",
    "pg": "8.5.1",
    "pino": "6.11.1",
    "pino-pretty": "4.5.0",
    "redis": "^4.0.3",
    "redis-om": "^0.1.7",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.16.1",
    "shared": "file:shared",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "9.1.1",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.11",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^14.14.31",
    "@types/nodemailer": "^6.4.4",
    "@types/pino": "6.3.6",
    "@types/redis": "^4.0.11",
    "@types/sequelize": "4.28.9",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "nodemon": "2.0.7",
    "sequelize-cli": "^6.2.0",
    "tsconfig-paths": "3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "4.1.2"
  }
}
